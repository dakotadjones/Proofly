{
  "expo": {
    "name": "proofly",
    "slug": "proofly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Proofly to access your camera to take job photos."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Proofly to access your photos.",
          "savePhotosPermission": "Allow Proofly to save photos to your device."
        }
      ],
      "expo-mail-composer"
    ]
  }
}
